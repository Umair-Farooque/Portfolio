{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://portfolio-backend-teqm.onrender.com"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}